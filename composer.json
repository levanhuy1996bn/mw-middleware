{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://gitlab.endertech.net/api/v4/group/3/-/packages/composer/packages.json"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-sodium": "*",
        "cweagans/composer-patches": "^1.7",
        "endertech/ecommerce-middleware-admin-bundle": "^1.7",
        "endertech/ecommerce-middleware-admin-events-bundle": "^1.7",
        "endertech/ecommerce-middleware-bundle": "^1.7",
        "endertech/ecommerce-middleware-notification-bundle": "^1.7",
        "endertech/ecommerce-middleware-notification-events-bundle": "^1.7",
        "endertech/ecommerce-middleware-report-inventory-admin-bundle": "^1.7",
        "endertech/ecommerce-middleware-report-metafield-admin-bundle": "^1.7",
        "endertech/ecommerce-middleware-report-price-admin-bundle": "^1.7",
        "endertech/ecommerce-middleware-shopify-storis-bundle": "^1.7",
        "endertech/ecommerce-middleware-shopify-storis-events-bundle": "^1.7",
        "endertech/ecommerce-middleware-user-admin-bundle": "^1.7",
        "endertech/ecommerce-middleware-variant-admin-bundle": "^1.7",
        "endertech/sonata-user-webauthn-bundle": "^1.0",
        "geocoder-php/google-maps-provider": "^4.6",
        "nelmio/cors-bundle": "^2.2",
        "nyholm/psr7": "^1.4",
        "phpseclib/phpseclib": "^3.0",
        "symfony/apache-pack": "^1.0",
        "symfony/console": "6.4.*",
        "symfony/dotenv": "6.4.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "6.4.*",
        "symfony/intl": "6.4.*",
        "symfony/runtime": "6.4.*",
        "symfony/webhook": "6.4.*",
        "symfony/yaml": "6.4.*",
        "willdurand/geocoder-bundle": "^5.16"
    },
    "require-dev": {
        "symfony/browser-kit": "6.4.*",
        "symfony/phpunit-bridge": "6.4.*"
    },
    "config": {
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "gitlab-domains": ["gitlab.endertech.net"],
        "platform": {
            "php": "8.1.11"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "test": "bin/phpunit",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "patches": {
            "sonata-project/admin-bundle": {
                "": "patches/sonata-project/admin-bundle.patch"
            }
        },
        "symfony": {
            "allow-contrib": true,
            "require": "6.4.*"
        }
    }
}
